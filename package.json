{
  "name": "typescript-parcel",
  "version": "1.0.0",
  "repository": "git@github.com:maroon8021/typescript-parcel.git",
  "author": "Mitsu",
  "license": "MIT",
  "scripts": {
    "dev": "npx parcel src/index.html --open",
    "build": "npx parcel build src/index.html --no-source-maps",
    "serve": "npx parcel dist/index.html --open",
    "test": "jest --silent=false",
    "prepare": "husky install",
    "lint": "eslint --cache './src/**/*.{ts,tsx}'",
    "format": "pretty-quick --staged"
  },
  "dependencies": {
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^7.0.0",
    "jest": "^26.0.1",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^26.0.0"
  }
}
